{
  "name": "my-new-project",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "testOnce": "node ./node_modules/jest/bin/jest.js",
    "checkFormat": "node node_modules/prettier/bin-prettier.js -l '**/*.tsx' '**/*.ts' '**/*.js'"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "dependencies": {
    "@expo/samples": "2.1.1",
    "@types/expo": "^31.0.2",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-native": "^0.57.27",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-navigation": "^3.0.0",
    "@types/react-test-renderer": "^16.0.3",
    "babel-jest": "^23.6.0",
    "expo": "^32.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-maps": "^0.22.1",
    "react-native-stars": "^1.1.5",
    "react-native-typescript-transformer": "^1.2.10",
    "react-navigation": "^3.0.9",
    "react-navigation-header-buttons": "^2.1.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.6"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.0.0",
    "jest-expo": "^31.0.0"
  }
}
